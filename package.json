  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "check:types": "tsc --noEmit",
    "postbuild": "next-sitemap"
